<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PassingIntents2">

    <TextView
        android:id="@+id/textStudentProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Student Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.009"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textFirstName"
        android:layout_width="91dp"
        android:layout_height="20dp"
        android:layout_marginTop="28dp"
        android:text="First Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textStudentProfile" />
    <TextView
        android:id="@+id/textLastName"
        android:layout_width="91dp"
        android:layout_height="20dp"
        android:layout_marginTop="28dp"
        android:text="Last Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textFirstName" />
    <TextView
        android:id="@+id/textGender"
        android:layout_width="91dp"
        android:layout_height="20dp"
        android:layout_marginTop="28dp"
        android:text="Gender"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textLastName" />
    <TextView
        android:id="@+id/textBirthdate"
        android:layout_width="91dp"
        android:layout_height="20dp"
        android:layout_marginTop="28dp"
        android:text="Birth Date"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textGender" />
    <TextView
        android:id="@+id/textEmailAddress"
        android:layout_width="91dp"
        android:layout_height="20dp"
        android:layout_marginTop="28dp"
        android:text="Email Address"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textBirthdate" />
</androidx.constraintlayout.widget.ConstraintLayout>